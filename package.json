  "license": "MIT",
  "devDependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "async": "^3.1.0",
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.4",
    "documentdb": "^1.15.3",
    "express": "^4.16.2",
    "jade": "^1.11.0",
    "morgan": "^1.9.1",
    "react-router-dom": "^5.1.2"
  }
}
